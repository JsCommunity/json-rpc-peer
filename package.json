{
  "name": "json-rpc-peer",
  "version": "0.11.0",
  "license": "ISC",
  "description": "JSON-RPC 2 transport-agnostic library",
  "keywords": [
    "json",
    "json-rpc",
    "jsonrpc",
    "jsonrpc2",
    "rpc"
  ],
  "homepage": "https://github.com/julien-f/js-json-rpc-peer",
  "bugs": "https://github.com/julien-f/js-json-rpc-peer/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-f/js-json-rpc-peer.git"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "json-rpc-protocol": "^0.10.0",
    "lodash.foreach": "^3.0.3",
    "lodash.isarray": "^3.0.3",
    "lodash.map": "^3.1.4",
    "readable-stream": "^2.0.2"
  },
  "devDependencies": {
    "babel": "^5.5.1",
    "babel-eslint": "^4.1.3",
    "clarify": "^1.0.5",
    "mocha": "^2.2.5",
    "must": "^0.13.1",
    "source-map-support": "^0.3.1",
    "standard": "^5.3.1",
    "trace": "^2.0.0"
  },
  "scripts": {
    "build": "babel --source-maps --out-dir=dist/ src/",
    "dev": "babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint",
    "prepublish": "npm run build",
    "test": "mocha --opts .mocha.opts \"dist/**/*.spec.js\"",
    "test-dev": "mocha --opts .mocha.opts --watch --reporter=min \"dist/**/*.spec.js\""
  },
  "standard": {
    "ignore": [
      "dist/**"
    ],
    "parser": "babel-eslint"
  }
}
